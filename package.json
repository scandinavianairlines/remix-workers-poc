{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "dev": "run-p 'dev:*'",
    "start": "remix-serve build",
    "typecheck": "tsc",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "build:worker": "esbuild ./app/entry.worker.js --outfile=./public/entry.worker.js --minify --bundle --platform=node --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "dev:poc": "nodemon --config ./nodemon.json scripts/build.js",
    "dev:remix": "cross-env NODE_ENV=development remix dev"
  },
  "dependencies": {
    "@remix-pwa/sw": "latest",
    "@remix-run/css-bundle": "^1.19.1",
    "@remix-run/node": "^1.19.1",
    "@remix-run/react": "^1.19.1",
    "@remix-run/router": "latest",
    "@remix-run/serve": "^1.19.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "isbot": "^3.6.8",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.19.1",
    "@remix-run/eslint-config": "^1.19.1",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "esbuild": "^0.18.17",
    "esbuild-plugins-node-modules-polyfill": "^1.3.0",
    "eslint": "^8.38.0",
    "nodemon": "^3.0.1",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "yarn@3.6.1"
}
